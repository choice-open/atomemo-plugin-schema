{
  "name": "@choiceopen/atomemo-plugin-schema",
  "version": "0.1.4",
  "description": "Schema for developing Choiceform atomemo plugins",
  "homepage": "https://github.com/choice-open/atomemo-plugin-schema#readme",
  "bugs": {
    "url": "https://github.com/choice-open/atomemo-plugin-schema/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/choice-open/atomemo-plugin-schema.git"
  },
  "license": "MIT",
  "author": "江志强 <zhiqiang.jiang@choiceform.com>",
  "type": "module",
  "exports": {
    "./schemas": "./dist/schemas.js",
    "./types": "./dist/types.js",
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "check": "biome check --write",
    "dev": "tsdown --watch",
    "prepublishOnly": "bun run build",
    "test": "bun test --dots",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "type-fest": "^5.4.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/bun": "^1.3.6",
    "bumpp": "^10.4.0",
    "tsdown": "^0.19.0",
    "typescript": "^5.9.3",
    "zod": "^4.3.5"
  },
  "peerDependencies": {
    "zod": "^4"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": false
    }
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./schemas": "./dist/schemas.js",
      "./types": "./dist/types.js",
      "./package.json": "./package.json"
    }
  }
}
